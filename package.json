{
  "name": "react-components",
  "version": "0.0.1",
  "description": "Repository for reusable react components",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --progress --watch",
    "start": "webpack-dev-server --port 9000 --open",
    "build": "./node_modules/.bin/eslint src/**/*.js --fix --ignore-path .eslintignore &&  webpack -p",
    "test": "jest",
    "testwatch": "./node_modules/.bin/eslint src/**/*.js --fix --ignore-path .eslintignore && jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markandan/react-components.git"
  },
  "keywords": [
    "ReactJS",
    "React",
    "Components"
  ],
  "author": "Markandan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/markandan/react-components/issues"
  },
  "homepage": "https://github.com/markandan/react-components#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.0.6",
    "node-sass": "^4.8.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2",
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.24.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "jest": "^21.2.1",
    "react-test-renderer": "^16.1.1",
    "regenerator-runtime": "^0.11.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
